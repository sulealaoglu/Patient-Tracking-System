@model List<LogViewModel>


<h2 color=darkorchid;> All Logs</h2>
<table class="table table-bordered">
  <thead>
    <tbody>
        @foreach (var p in Model)
        {
            <tr>
                <th scope="col">@p.username</th>
                <th scope="col">@p.DateTime</th>
    </tr>
        }
    
  </thead>
  <tbody>
  </tbody>
</table>