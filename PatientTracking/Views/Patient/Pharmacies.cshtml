@{
    @model List<Pharmacy>
   
        <div class="row">
        @foreach (var p in Model)
        {<br></br>
            <div class="col-sm-6">
                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title">@p.name</h5>
                        <p class="card-text">Address ; @p.address</p>
                    </div>
                    
                </div>
            </div>
        }
    </div>
}
